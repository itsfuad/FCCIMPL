// Test reference types (C# style)

type Point struct {
    .x: i32,
    .y: i32,
};

// Test reference parameter
fn modifyPoint(p: &Point) {
    p.x = 100;
}

// Test reference return type
fn getReference() -> &Point {
    let p := Point { .x = 1, .y = 2 };
    return p;
}

// Test reference variable type
fn testRefVar() {
    let p := Point { .x = 1, .y = 2 };
    let ref: &Point = p;
}
