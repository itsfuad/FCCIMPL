// Test struct field access

type Point struct {
    .x: i32,
    .y: i32
};

type Line struct {
    .start: Point,
    .end: Point
};

fn test() {
    // Struct literal
    let p: Point = Point{
        .x = 10,
        .y = 20
    };

    // Simple field access
    let x_val: i32 = p.x;
    let y_val: i32 = p.y;

    let line: Line = Line{
        .start = Point{ .x = 0, .y = 0 },
        .end = Point{ .x = 10, .y = 10 }
    };

    // Chained field access
    let start_x: i32 = line.start.x;
    let end_y: i32 = line.end.y;
}

